    <AbsoluteLayout
        android:layout_width="380dp"
        android:layout_height="542dp"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="-5dp"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context="com.student.portretov.lab2.MainActivity">

        <Button
        android:id="@+id/btnC"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="8dp"
        android:layout_y="322dp"
        android:text="C"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="360dp" />

    <EditText
        android:id="@+id/mainEditText"
        android:layout_width="350dp"
        android:layout_height="97dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="38dp"
        android:layout_x="7dp"
        android:layout_y="9dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnEquals"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

        <Button
        android:id="@+id/btn1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_x="8dp"
        android:layout_y="112dp"
        android:text="1"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/btn4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainEditText"
        app:layout_constraintVertical_bias="0.0" />

        <Button
        android:id="@+id/btn4"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="8dp"
        android:layout_y="182dp"
        android:text="4"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="220dp" />

        <Button
        android:id="@+id/btn5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="78dp"
        android:layout_y="182dp"
        android:text="5"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="220dp" />

        <Button
        android:id="@+id/btn3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="16dp"
        android:layout_x="148dp"
        android:layout_y="112dp"
        android:text="3"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/mainEditText"
        tools:layout_editor_absoluteX="157dp" />

        <Button
        android:id="@+id/btn2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="17dp"
        android:layout_x="78dp"
        android:layout_y="112dp"
        android:text="2"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/btn5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/btn1"
        app:layout_constraintRight_toLeftOf="@+id/btn3"
        app:layout_constraintTop_toBottomOf="@+id/mainEditText"
        app:layout_constraintVertical_bias="0.125" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="148dp"
        android:layout_y="182dp"
        android:text="6"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="220dp" />

        <Button
        android:id="@+id/btnEquals"
        android:layout_width="140dp"
        android:layout_height="138dp"
        android:layout_alignEnd="@+id/mainEditText"
        android:layout_below="@+id/mainEditText"
        android:layout_marginTop="15dp"
        android:layout_x="218dp"
        android:layout_y="112dp"
        android:text="="
        android:textSize="24sp"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="152dp" />

        <Button
        android:id="@+id/btn7"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignBaseline="@+id/btn8"
        android:layout_alignBottom="@+id/btn8"
        android:layout_toStartOf="@+id/btn8"
        android:layout_x="8dp"
        android:layout_y="252dp"
        android:text="7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="290dp" />

        <Button
        android:id="@+id/btn8"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignBaseline="@+id/btn9"
        android:layout_alignBottom="@+id/btn9"
        android:layout_toStartOf="@+id/btn9"
        android:layout_x="78dp"
        android:layout_y="252dp"
        android:text="8"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="290dp" />

        <Button
        android:id="@+id/btn9"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_below="@+id/btnEquals"
        android:layout_toStartOf="@+id/btnEquals"
        android:layout_x="148dp"
        android:layout_y="252dp"
        android:text="9"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="290dp" />

        <Button
        android:id="@+id/btn0"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="78dp"
        android:layout_y="322dp"
        android:text="0"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="360dp" />

        <Button
        android:id="@+id/btnArrow"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="148dp"
        android:layout_y="322dp"
        android:text="←"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="360dp" />

    <Button
        android:id="@+id/btnPlus"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="218dp"
        android:layout_y="252dp"
        android:text="+"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="290dp" />

        <Button
        android:id="@+id/btnMinus"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignEnd="@+id/btnEquals"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="79dp"
        android:layout_marginEnd="12dp"
        android:layout_x="288dp"
        android:layout_y="252dp"
        android:text="-"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="290dp" />

    <Button
        android:id="@+id/btnStar"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="218dp"
        android:layout_y="322dp"
        android:text="*"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="360dp" />

    <Button
        android:id="@+id/btnM"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="218dp"
        android:layout_y="392dp"
        android:text="M"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="360dp" />

    <Button
        android:id="@+id/btnV"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="148dp"
        android:layout_y="392dp"
        android:text="√"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="360dp" />

    <Button
        android:id="@+id/btnX"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_x="8dp"
        android:layout_y="392dp"
        android:text="Х²"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="360dp" />

    <Button
    android:id="@+id/btnDivine"
    android:layout_width="70dp"
    android:layout_height="70dp"
    android:layout_x="288dp"
    android:layout_y="322dp"
    android:text="/"
    android:textSize="24sp"
    tools:layout_editor_absoluteX="298dp"
    tools:layout_editor_absoluteY="360dp" />

    <Button
        android:id="@+id/btnMC"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_x="288dp"
        android:layout_y="392dp"
        android:text="MC"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="360dp" />

</AbsoluteLayout>


